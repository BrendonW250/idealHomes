<!DOCTYPE html>
<html lang="en">
	<head>
		<title>Generic - Phantom by HTML5 UP</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="/css/main.css" />
		<noscript><link rel="stylesheet" href="/css/noscript.css" /></noscript>
	</head>
	<body class="is-preload">
        <%- include('partials/header') -%>
		<!-- Wrapper -->
			<div id="wrapper">

		

				<!-- Main -->
					<div id="main">
						<div class="inner">
							<h1>Bainbridge Nursing & Rehabilitation Center</h1>
							<!-- <span class="image main"><img src="images/pic13.jpg" alt="" /></span> -->
                            <p>
                            <!-- <% for (let i=0; i<homes.length; i++){%>
                                <% if (homes[i].name == 'Bainbridge Nursing & Rehabilitation Center'){%>
                                    <h1><%= homes[i].name %></h1>
                                <%}%>
                            <%}%> -->
                            </p>
                            <!-- Displaying the bainbridge home address -->
                            <p>
                            <% for (let i=0; i<homes.length; i++){%>
                                <% if (homes[i].address == '3518 Bainbridge Ave, Bronx, NY 10467'){%>
                                    <h2>Address: <%= homes[i].address %></h2>
                                <%}%>
                            <%}%>
                            </p>
                        
                            <!-- Displaying the nursing home's phone number -->
                            <p>
                            <% for (let i=0; i<homes.length; i++){%>
                                <% if (homes[i].phoneNumber == '718-655-1991'){%>
                                    <h2>Contact: <%= homes[i].phoneNumber %></h2>
                                <%}%>
                            <%}%>
                            </p>
                            
                        
                            <!-- Displaying the nursing home's website -->
                            <p>
                            <% for (let i=0; i<homes.length; i++){%>
                                <% if (homes[i].website == 'bainbridgecares.com'){%>
                                    <a href="https://bainbridgecares.com">Website</a>
                                    
                                <%}%>
                            <%}%>
                            </p>

                            <div class="col-6 mt-5">
                                <a class="btn btn-primary" href="/profile">Profile</a>
                                <a class="btn btn-primary" href="/home">Home</a>
                            </div>
							
						</div>
					</div>


			</div>

            <%- include('partials/footer') -%>
		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="assets/js/main.js"></script>

	</body>
</html>




  

